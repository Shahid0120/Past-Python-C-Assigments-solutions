#include <stdio.h> 

int main(void) {
    int array[10] = {0};
    int i = 0;
    while ( i < 10) {
        scanf("%d", &array[i]);
        i++;
    }
    
    printf("Odd numbers were: ");
    i = 0;
    while ( i < 10) {
        if ( array[i] % 2 == 1) {
            printf("%d ", array[i]);      
        }
        i++;
    }
    printf("\n");
    
    
    
    printf("Even numbers were: ");
    i = 0;
    while ( i < 10) {
        if ( array[i] % 2 == 0) {
        printf("%d ", array[i]);
        
        }
        i++;   
    }
    printf("\n");
    
    



}